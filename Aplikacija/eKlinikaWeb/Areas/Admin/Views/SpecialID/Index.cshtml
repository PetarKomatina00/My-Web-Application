@model IEnumerable<IDSpecial>
@{
    ViewData["Title"] = "Index";
}
<div class = "container p-3">
    <div class = "row pt-4">
        <div class = "col-6">
            <h2 class = "text-primary">
                Lista Pacijenata
            </h2>
        </div>
        <div class = "text-end pb-3">
            <a asp-controller = "SpecialID" asp-action = "Create" class = "btn btn-primary">
                <i class = "bi bi-plus=circle"> </i>
                Napravi novi ID
            </a>
        </div>
    </div>

    <table class = "table table-bordered table-striped" style = "width:100%">
        <thead>
            <tr>
                <th width = "25%">
                    SpecialIDNum
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var x in Model)
            {
                <tr>
                    <td width = "25%">
                        @x.SpecialIDNum
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>